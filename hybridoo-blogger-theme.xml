    </b:widget-settings>
    <b:includable id='main' var='top'>
       <b:if cond='data:mobile == &quot;false&quot;'>
          <!-- posts -->
          <div class='blog-posts hfeed'>
             <b:include data='top' name='status-message'/>
             <data:defaultAdStart/>
             <b:loop values='data:posts' var='post'>
                <div class='post-outer'>
                   <b:include data='post' name='post'/>
                </div>
             </b:loop>
             <data:adEnd/>
          </div>
          <div style='clear: both;'/>
       </b:if>
       <!-- end of data:mobile == &quot;false&quot; -->
    </b:includable>

    <!-- this is important include -->
    <b:includable id='post' var='post'>
       <article class='post hentry'>
          <!-- post article metadata info -->
          <!--
          <b:if cond='data:blog.pageType != &quot;static_page&quot;'>
             <div class='bpostmeta'>
                <span>
                   <i class='fa fa-clock-o'/> 
                   <data:post.timestamp/>
                </span>
             </div>
          </b:if>
        -->
          <!-- article details -->
          <div class='post-body entry-content clearfix'>
              <a expr:name='data:post.id'/>
              <!-- end homepage i.e. index page-->
              <!-- when page is not a home page and not in preview mode -->
              <b:if cond='data:blog.url != data:blog.homepageUrl'>
                <h1 class='post-title entry-title'>
                  <b:if cond='data:post.link'>
                    <a expr:href='data:post.link'><data:post.title/></a>
                    <b:else/>
                    <b:if cond='data:post.url'>
                        <a expr:href='data:post.url'><data:post.title/></a>
                        <b:else/>
                        <data:post.title/>
                    </b:if>
                  </b:if>
                </h1>
                <div class='entry-colors'>
                <div class='color_col_1'/>
                <div class='color_col_2'/>
                <div class='color_col_3'/></div>
              </b:if>
              <!-- post item in preview mode -->
              <b:if cond='data:blog.url == data:blog.homepageUrl'>
                <b:if cond='data:blog.pageType == &quot;item&quot;'>
                    <h1 class='post-title entry-title'>
                      <b:if cond='data:post.link'>
                        <a expr:href='data:post.link'><data:post.title/></a>
                        <b:else/>
                        <b:if cond='data:post.url'>
                            <a expr:href='data:post.url'><data:post.title/></a>
                            <b:else/>
                            <data:post.title/>
                        </b:if>
                      </b:if>
                    </h1>
                    <div class='entry-colors'>
                    <div class='color_col_1'/>
                    <div class='color_col_2'/>
                    <div class='color_col_3'/></div>
                </b:if>
              </b:if>
              <b:if cond='data:blog.pageType == &quot;item&quot;'>
                 <data:post.body/>
              </b:if>
              <b:if cond='data:blog.pageType == &quot;static_page&quot;'>
                 <data:post.body/>
              </b:if>
              <div style='clear: both;'/>
                 <!-- clear for photos floats -->
          </div>
          <!-- post article metadata info incase of detail mode display more metadata info -->
          <b:if cond='data:blog.pageType != &quot;static_page&quot;'>
             <b:if cond='data:blog.pageType == &quot;item&quot;'>
                <div class='bpostmeta'>
                   <b:if cond='data:post.labels'>
                      <span>
                         <i class='fa fa-bookmark'/> 
                         <b:loop values='data:post.labels' var='label'>
                            <a expr:href='data:label.url' rel='tag'>
                               <data:label.name/>
                            </a>
                            <b:if cond='data:label.isLast != &quot;true&quot;'>, </b:if>
                         </b:loop>
                      </span>
                   </b:if>
                   <b:if cond='data:post.allowComments'>
                      <span>
                         <i class='fa fa-comments'/> 
                         <a expr:href='data:post.addCommentUrl' expr:onclick='data:post.addCommentOnclick'>
                            <b:if cond='data:post.numComments == 0'>No comments</b:if>
                            <b:if cond='data:post.numComments == 1'>1 comment</b:if>
                            <b:if cond='data:post.numComments &gt;= 2'>
                               <data:post.numComments/>
                               comments
                            </b:if>
                         </a>
                      </span>
                   </b:if>
                </div>
             </b:if>
          </b:if>
       </article>
    </b:includable>
    <!-- end of important include -->
  </b:widget>
</b:section>
